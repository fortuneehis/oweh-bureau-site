<!-- How it works Section Start -->
<div class="full-row pt-5">
	<div class="container">
		<div class="row">
			<div class="">
				<form action="#" method="POST">
					<div class="row">
						<div class="col-lg-12 mb-4">
							<h5 class="down-line">Get Quote for <%= query['id'] %></h5>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<input type="text" class="form-control bg-gray" name="name" value="" placeholder="Full Name">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<input type="text" class="form-control bg-gray" name="phone" value="" placeholder="Phone Number">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<input type="text" class="form-control bg-gray" name="email" value="" placeholder="Email Address">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<input type="text" class="form-control bg-gray" name="subject" value="" placeholder="Company (if applicable)">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<input type="text" class="form-control bg-gray" name="date" value="" placeholder="Prefered Consultation Date">
							</div>
						</div>
						<div class="col-md-6">
							<div class="form-group">
								<input type="text" class="form-control bg-gray" name="time" value="" placeholder="Prefered Consultation Time">
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group">
								<textarea class="form-control bg-gray" cols="30" rows="6" placeholder="Brief Description of Your Requirements"></textarea>
							</div>
						</div>
						<div class="col-md-12"> <a class="btn btn-primary" href="#">GET QUOTE</a> </div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>
<!-- How it works Section End -->
